<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="connectorExecutor" class="com.bonitasoft.engine.connector.impl.ConnectorExecutorTimedOut">
		<constructor-arg name="queueCapacity" value="25" />
		<constructor-arg name="corePoolSize" value="<%= scope.lookupvar('bonita_bpm::params::corePoolSize') %>" />
		<constructor-arg name="loggerService" ref="tenantTechnicalLoggerService" />
		<!-- timeout in seconds, default = 5 minutes -->
		<constructor-arg name="timeout" value="<%= scope.lookupvar('bonita_bpm::params::connectorTimeout') %>" />
		<constructor-arg name="maximumPoolSize" value="<%= scope.lookupvar('bonita_bpm::params::maximumPoolSize') %>" />
		<constructor-arg name="keepAliveTimeSeconds" value="100" />
		<constructor-arg name="sessionAccessor" ref="sessionAccessor" />
		<constructor-arg name="sessionService" ref="sessionService" />
        <constructor-arg name="timeTracker" ref="timeTracker" />
	</bean>

</beans>